<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/appStandardColor"
    tools:context=".SafetySeekBarActivity"
    android:orientation="vertical">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/safetySeekBarLinearLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:paddingTop="5dp"
        android:paddingBottom="5dp">

        <FrameLayout
            android:id="@+id/safetySeekBarFrame"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_gravity="top"
            android:paddingStart="24dp"
            android:paddingEnd="25dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/btnBx"
            app:layout_constraintStart_toStartOf="parent"
            >

            <View
                android:layout_width="wrap_content"
                android:layout_height="6dp"
                android:layout_gravity="center_vertical"
                android:background="@drawable/seekbar_indicator"
                android:layout_marginLeft="45dp"
                android:layout_marginRight="25dp"/>

            <View
                android:layout_width="22dp"
                android:layout_height="22dp"
                android:layout_gravity="center_vertical|right"
                android:layout_margin="10dp"
                android:background="@drawable/seekbar_nub"/>

            <SeekBar
                android:id="@+id/safetySeekBarH"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:paddingStart="25dp"
                android:paddingEnd="25dp"
                android:layout_gravity="top"
                android:layoutDirection="rtl"
                android:max="255"
                android:maxHeight="7dp"
                android:minHeight="7dp"
                android:progress="255"
                android:progressDrawable="@drawable/safety_seekbar_style"
                android:thumb="@drawable/safety_seekbar_locked_thumb" />

        </FrameLayout>

        <Button
            android:id="@+id/btnBx"
            android:layout_width="48dp"
            android:layout_height="wrap_content"
            android:includeFontPadding="false"
            android:text="Bx"
            android:textSize="12sp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/btnTx"
            />

        <Button
            android:id="@+id/btnTx"
            android:layout_width="48dp"
            android:layout_height="wrap_content"
            android:includeFontPadding="false"
            android:text="Tx"
            android:textSize="12sp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            />

    </androidx.constraintlayout.widget.ConstraintLayout>

